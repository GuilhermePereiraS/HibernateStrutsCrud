<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 2.0//EN"
    "https://hibernate.org/dtd/hibernate-mapping-2.0.dtd">

<hibernate-mapping>
    <class name="cba.ifmt.forms.UsuarioForm" table="Usuarios">
        <id name="id" column="id">
            <generator class="native"/>
        </id>
        <property name="nome" column="nome"/>
        <property name="email" column="email"/>
        <property name="cpf" column="cpf"/>
        
        <many-to-one name="id" class="cba.ifmt.forms.MunicipioBean" column="municipio_id" not-null="true"></many-to-one>
    </class>
</hibernate-mapping>